
@model List<MVCApp.Models.CategoryViewModels.CategoryViewModel>

@addTagHelper *, MVCApp

@{
    ViewData["Title"] = "List category";
}

<h2>@ViewData["Title"]</h2>

<div class="row">
    <div class="col-md-4">
        <list-category for-categories="@Model"></list-category>
    </div>
</div>

@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}




